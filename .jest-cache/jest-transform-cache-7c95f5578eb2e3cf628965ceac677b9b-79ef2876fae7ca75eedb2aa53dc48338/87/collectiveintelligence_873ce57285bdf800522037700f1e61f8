        this.onCollectiveMutation(mutationId);
      } else
      /* istanbul ignore next */
      {
        cov_2c31ar482u().b[5][1]++;
      }
      cov_2c31ar482u().s[26]++;
      return true;
    } else {
      /* istanbul ignore next */
      cov_2c31ar482u().b[4][1]++;
      cov_2c31ar482u().s[27]++;
      console.log(`[Collective] Consensus non atteint pour ${mutationId} (${votes}/${totalPeers})`);
      /* istanbul ignore next */
      cov_2c31ar482u().s[28]++;
      return false;
    }
  }
}
